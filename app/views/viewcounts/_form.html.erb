<%= form_with(model: viewcount) do |form| %>
  <% if viewcount.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(viewcount.errors.count, "error") %> prohibited this viewcount from being saved:</h2>

      <ul>
        <% viewcount.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :stream_id %>
    <%= form.number_field :stream_id %>
  </div>

  <div class="field">
    <%= form.label :viewers %>
    <%= form.number_field :viewers %>
  </div>

  <div class="field">
    <%= form.label :created_at %>
    <%= form.datetime_select :created_at %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
